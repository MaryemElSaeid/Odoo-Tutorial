<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
     <record id="itinew.basic_model2_report_view" model="ir.actions.report">
       <field name="name">Model 1</field>
       <field name="model">itinew.basic.model2</field>
       <field name="report_type">qweb-pdf</field>
       <field name="report_name">itinew.report_model1_view</field>
       <field name="report_file">itinew.report_model1_view</field>
       <field name="attachment_use" eval="False"/>
       <field name="binding_model_id" ref="model_itinew_basic_model2"/>
       <field name="binding_type">report</field>       
     </record>

     <template id="report_model1_view">
       <t t-call="web.html_container">
          <t t-foreach="docs" t-as="doc">
              <t t-call="web.external_layout">
                  <div class="page">
                        <h2><span t-field="doc.name"/> </h2>
                        <h2><span t-field="doc.unit_price"/> </h2>
                        <h2><span t-field="doc.qty"/> </h2>
                        <h2><span t-field="doc.model1"/> </h2>
                        <h2><span t-field="doc.order"/> </h2>
                  </div>
              </t>
          </t>
       </t>
     </template>
  </data>
</odoo>

